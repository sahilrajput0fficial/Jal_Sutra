{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(login|add-data|health)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*\\.(html|js|css|png|jpg|gif|svg|ico))",
      "dest": "/$1"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "api/index.js": {
      "maxDuration": 10
    }
  }
}
